<ui:UiBinder 
	xmlns:ui='urn:ui:com.google.gwt.uibinder' 
	xmlns:g='urn:import:com.google.gwt.user.client.ui' 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" 
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	ui:baseMessagesInterface="${ROOT_PKG}.client.resources.i18n.Messages">

	<!-- File generated by $generator.name ( version $generator.version ) - Date $today.date("yyyy-MM-dd") ( Time $today.time ) -->

	<ui:with field="res" type="${ROOT_PKG}.client.resources.AppResources" />

	<g:HTMLPanel ui:field="container">
		<b:Navbar position="FIXED_BOTTOM" addStyleNames="{res.style.footerBar}">
			<b:Container>
				<b:Row>
					<b:Column size="MD_6">
						<b:NavbarNav>
#foreach( $entity in $selectedEntities )
#if ($foreach.index > 8)
#set( $uncapitalizedEntityName = $fn.uncapitalize($entity.name) )
							<b:AnchorListItem ui:field="${uncapitalizedEntityName}" text="${entity.name}">
								<ui:attribute name='text' key="layout.${uncapitalizedEntityName}" />
							</b:AnchorListItem>
#end
#end
						</b:NavbarNav>
					</b:Column>
					<b:Column size="MD_6">
						<b:NavbarText pull="RIGHT">
							<b.html:Span>
								<g:InlineLabel text="Powered by Telosys 3 -">
									<ui:attribute name='text' key="layout.powered.by" />
								</g:InlineLabel>
								<b:NavbarLink text="Visit our site for more information" href="https://github.com/telosys-tools-bricks">
									<ui:attribute name='text' key="layout.telosys.link" />
								</b:NavbarLink>
							</b.html:Span>
						</b:NavbarText>
					</b:Column>
				</b:Row>
			</b:Container>
		</b:Navbar>
	</g:HTMLPanel>
</ui:UiBinder>