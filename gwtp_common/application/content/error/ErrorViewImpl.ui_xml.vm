<ui:UiBinder 
	xmlns:ui='urn:ui:com.google.gwt.uibinder' 
	xmlns:g='urn:import:com.google.gwt.user.client.ui' 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

	<!-- File generated by $generator.name ( version $generator.version ) - Date $today.date("yyyy-MM-dd") ( Time $today.time ) -->

	<ui:with field="res" type="${ROOT_PKG}.client.resources.AppResources" />

	<g:HTMLPanel>
		<b:Container>
			<g:HTMLPanel>
				<b:Column size="MD_12" addStyleNames="text-center">
					<b:Image type="CIRCLE" resource="{res.error}" />
				</b:Column>
				<b:Column size="MD_12" addStyleNames="text-center text-danger">
					<b:Heading size="H1" text="An error occured, please contact website administrator" />
					<g:Label text="For reduce the response time, please provide the time of occurence"></g:Label>
				</b:Column>
			</g:HTMLPanel>
		</b:Container>
	</g:HTMLPanel>
</ui:UiBinder>